{
  "name": "uBugger",
  "version": "1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "icons": {
    "128": "lib/debugger/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["lib/debugger/launch.js"],
      "transient": true
    }
  },
  "permissions": [
    "experimental", {
      "socket": [
        "tcp-connect:*:*"
      ]
    }
  ]
}
